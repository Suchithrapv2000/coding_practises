public class Stocks{
    public int maxProfit(int[] prices) {
        int maxPro = 0;
        int minPrice = Integer.MAX_VALUE;
        boolean isProfit = false;
        
        for(int i=0;i<prices.length;i++){
            if(prices[i] < minPrice){
                minPrice = prices[i];
            }else{
                if(maxPro < prices[i]-minPrice){
                    maxPro = prices[i]-minPrice;
                }
            }
            
        }
        return maxPro;
    }
}